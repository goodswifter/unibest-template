  },
  "homepage": "https://feige996.github.io/unibest/",
  "engines": {
    "node": ">=18",
    "pnpm": ">=7.30"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "uvm": "npx @dcloudio/uvm@latest",
    "uvm-rm": "node ./scripts/post-upgrade.js",
    "postuvm": "echo upgrade uni-app success!",
    "dev:app": "uni -p app",
    "dev:app-android": "uni -p app-android",
    "dev:app-ios": "uni -p app-ios",
    "dev": "uni --mode dev",
    "dev:h5": "uni --mode dev",
    "dev:uat": "uni --mode uat",
    "dev:prod": "uni --mode prod",
    "dev:mp": "uni -p mp-weixin",
    "dev:mp-uat": "uni -p mp-weixin --mode uat",
    "dev:mp-prod": "uni -p mp-weixin --mode prod",
    "dev:mp-weixin": "uni -p mp-weixin",
    "build:app": "uni build -p app",
    "build:app-android": "uni build -p app-android",
    "build:app-ios": "uni build -p app-ios",
    "build": "uni build --mode dev",
    "build:h5": "uni build --mode dev",
    "build:uat": "uni build --mode uat",
    "build:prod": "uni build --mode prod",
    "build:mp": "uni build -p mp-weixin --mode dev",
    "build:mp-uat": "uni build -p mp-weixin --mode uat",
    "build:mp-prod": "uni build -p mp-weixin --mode prod",
    "build:mp-weixin": "uni build -p mp-weixin --mode dev",
    "prepare": "git init && husky install",
    "type-check": "vue-tsc --noEmit",
    "cz": "czg",
    "openapi-ts-request": "openapi-ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "**/*.{html,vue,ts,cjs,json,md}": [
      "prettier --write"
    ],
    "**/*.{vue,js,ts,jsx,tsx}": [
      "eslint --cache --fix"
    ],
    "**/*.{vue,css,scss,html}": [
      "stylelint --fix"
    ]
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
